    "@types/node": "^14.6.2",
    "@types/sequelize": "^4.28.9",
    "apollo-server-express": "^2.17.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
